{
  "name": "properties-manager-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node --enable-source-maps dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "dev:setup": "docker-compose up -d && sleep 5 && pnpm db:migrate --name init && pnpm db:seed",
    "dev:db": "docker-compose up -d",
    "dev:db:stop": "docker-compose down",
    "dev:test": "./scripts/test-setup.sh",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "check": "pnpm lint && pnpm format:check && pnpm test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@prisma/client": "^6.15.0",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "graphql": "^16.11.0",
    "pg": "^8.16.3",
    "prisma": "^6.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ]
  }
}
